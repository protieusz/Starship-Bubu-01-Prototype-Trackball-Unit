{
    "keyboard_name": "starshipbubu02",
    "manufacturer": "protieuskeebs",
    "url": "https://instagram.com/protieuskeebs/",
    "maintainer": "protieus",
    "usb": {
        "vid": "0xFEED",
        "pid": "0x0000",
        "device_version": "1.0.0"
    },
    "processor": "RP2040",
    "bootloader": "rp2040",
    "processor_type": "arm",
    "protocol": "ChibiOS",
    "debounce": 5,
    "matrix_pins": {
        "rows": ["GP15", "GP14"],
        "cols": ["GP0", "GP12", "GP13"]
    },
    "matrix_size": {
        "cols": 3,
        "rows": 2
    },
    "encoder": {
        "rotary": [
            { "pin_a": "GP4", "pin_b": "GP5", "resolution": 4}
        ]
    },
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "encoder": true,
        "rgblight": true
    },

    //"rgblight": {
