 "layouts": {
        "LAYOUT": {
            "layout": [
                { "label": "K000", "matrix": [0, 0], "w": 1, "x": 0, "y": 0 },
                { "label": "K002", "matrix": [0, 2], "w": 1, "x": 1, "y": 0 },
                { "label": "K100", "matrix": [1, 0], "w": 1, "x": 2, "y": 0 },
                { "label": "K101", "matrix": [1, 1], "w": 1, "x": 3, "y": 0 },
                { "label": "K102", "matrix": [1, 2], "w": 1, "x": 4, "y": 0 }
            ]
        }
    }
